/* ////////////////////////////////////////////////////////
SideBar QWidget  */

#left_sideBar{
	background-color: rgb(23, 21, 59);
	color: rgb(203, 192, 247);
}
#left_sideBar QPushButton {
	padding:10px;
	background-color: transparent;
}

#left_sideBar  QPushButton::checked{
	background-color: rgb(56, 49, 130);
	color: rgb(0, 170, 127);
	border-radius:3px;
}

#main_nav{
	background-color:rgb(18, 15, 45);
	color: rgb(124, 132, 219);
}
#piechart_view{
	background-color: rgb(44, 38, 100);
}
#top_container{
	background-color: rgb(38, 33, 91);
	border-radius:5px;
}

/* ////////////////////////////////////////////////////////
Main Nav QPushButton  */

#minimize_btn, #restore_btn, #close_btn{
	background-color:transparent;
	border:0;
}
#close_btn:hover{
	background-color:rgb(235, 59, 90);
	color: rgb(255, 255, 255);
}
/* ////////////////////////////////////////////////////////
SearchBox QLineEdit  */

#searchBox {
	image: url(:/png/icons/loupe.png);
	image-position:right;
	border:1px solid rgb(58, 52, 139);
	padding:5px;
	background-color: transparent;
	color: rgb(168, 152, 247);
	border-radius:5px;
}
#searchBox:focus{
    border: 1px solid #006080;
}

/* ////////////////////////////////////////////////////////
QTableWidget */

QTableWidget{
	alternate-background-color: rgb(51, 45, 121);
	background-color:rgb(38, 33, 91);
	border-radius:10px;
    outline: none;
	padding: 10px;
}
QHeaderView::section:horizontal
{
    border: 1px solid rgb(33, 37, 43);
	background-color:rgb(23, 21, 59);
	color: rgb(134, 136, 223);
	height:35px;
	max-width: 30px;
	padding: 3px;
	border-top-left-radius: 7px;
    border-top-right-radius: 7px;
}
QTableWidget::item{
	border-color: rgb(44, 49, 60);
	color: rgb(170, 146, 236);
	padding-left: 5px;
	padding-right: 5px;
	gridline-color: rgb(44, 49, 60);
}
QTableWidget::item:selected{
	background-color:rgb(13, 146, 244);
	color: rgb(255, 255, 255);
}

/* ////////////////////////////////////////////////////////
ScrollBars */
QScrollBar:horizontal {
    border: none;
    background: rgb(51, 45, 121);
    height: 8px;
    margin: 0px 21px 0 21px;
	border-radius: 0px;
}
QScrollBar::handle:horizontal {
    background: rgb(23, 21, 59);
    min-width: 25px;
	border-radius: 4px
}
QScrollBar::add-line:horizontal {
    border: none;
    background: rgb(71, 78, 147);
    width: 20px;
	border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
    border: none;
    background: rgb(71, 78, 147);
    width: 20px;
	border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal
{
     background: none;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
     background: none;
}
 QScrollBar:vertical {
	border: none;
    background: rgb(51, 45, 121);
    width: 8px;
    margin: 21px 0 21px 0;
	border-radius: 0px;
 }
 QScrollBar::handle:vertical {	
	background: rgb(23, 21, 59);
    min-height: 25px;
	border-radius: 4px
 }
 QScrollBar::add-line:vertical {
     border: none;
    background: rgb(71, 78, 147);
     height: 20px;
	border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }
 QScrollBar::sub-line:vertical {
	border: none;
    background: rgb(71, 78, 147);
     height: 20px;
	border-top-left-radius: 4px;
    border-top-right-radius: 4px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     background: none;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }</string>
